   await analytics.setUserProperty({\n        name: 'planId',\n        value: result.currentPlan.planId,\n      });\n    }\n    const gaScrubbedURL = event?.urlAfterRedirects?.replace('#', '');\n    if (event.url) {\n      const gaObject = {\n        page_location: '/myVoyage' + event.url,\n        userId: result?.partyId,\n      };\n      await analytics.logEvent({\n        name: 'virtualPageView',\n        params: gaObject,\n      });\n    }\n    gaScrubbedURL &&\n      (await analytics.setScreenName({\n        screenName: gaScrubbedURL,\n      }));\n  }\n\n  async getQualtricsUser(endpointURL?: string): Promise<QualtricsUserProps> {\n    if (!this.QualtricsUserContent) {\n      const response = await this.baseService.get(\n        endpointURL ? endpointURL : this.endPoints.qualtricsUserProps\n      );\n      this.QualtricsUserContent = response;\n    }\n    return this.QualtricsUserContent;\n  }\n}\n","import {Injectable} from '@angular/core';\nimport {Observable} from 'rxjs';\nimport {EventManagerService} from '@shared-lib/modules/event-manager/event-manager/event-manager.service';\nimport {Publisher} from '@shared-lib/modules/event-manager/publisher';\nimport {HeaderInfo} from '@shared-lib/models/headerInfo.model';\n\n@Injectable({\n  providedIn: 'root',\n})\nexport class HeaderTypeService {\n  private headerInfoPublisher: Publisher;\n\n  constructor(private eventManagerService: EventManagerService) {\n    this.headerInfoPublisher = this.eventManagerService.createPublisher(\n      'headerInfo'\n    );\n  }\n\n  createSubscriber(): Observable<HeaderInfo> {\n    return this.eventManagerService.createSubscriber('headerInfo');\n  }\n\n  publish(headerInfo: HeaderInfo) {\n    this.headerInfoPublisher.publish(headerInfo);\n  }\n}\n","import {Injectable} from '@angular/core';\nimport {\n  Journey,\n  JourneyStep,\n  StepContentElement,\n  StepContentElements,\n  ValidationRuleInfo,\n  Option,\n} from '../models/journey.model';\nimport {BaseService} from '../../base/ba