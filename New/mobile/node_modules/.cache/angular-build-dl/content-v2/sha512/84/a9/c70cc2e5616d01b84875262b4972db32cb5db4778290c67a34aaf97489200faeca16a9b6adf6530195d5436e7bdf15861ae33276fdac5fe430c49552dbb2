let EditAccountRecoveryPage = class EditAccountRecoveryPage {
    constructor(headerType, accountInfoService, router) {
        this.headerType = headerType;
        this.accountInfoService = accountInfoService;
        this.router = router;
        this.displayText = _constants_recoveryText_json__WEBPACK_IMPORTED_MODULE_5__;
        this.actionOption = {
            headername: this.displayText.actionOption.header,
            btnleft: true,
            btnright: true,
            buttonLeft: {
                name: '',
                link: this.displayText.actionOption.buttonLeft,
            },
            buttonRight: {
                name: '',
                link: this.displayText.actionOption.buttonRight,
            },
        };
        this.editRecoveryForm = new _angular_forms__WEBPACK_IMPORTED_MODULE_6__.FormGroup({
            listOptions: new _angular_forms__WEBPACK_IMPORTED_MODULE_6__.FormControl(),
        });
    }
    ionViewWillEnter() {
        this.headerType.publish({
            type: _shared_lib_constants_headerType_enum__WEBPACK_IMPORTED_MODULE_3__.HeaderType.navbar,
            actionOption: this.actionOption,
        });
    }
    ngOnInit() {
        this.fetchScreenContent();
    }
    fetchScreenContent() {
        this.moreContentSubscription = this.accountInfoService
            .getScreenMessage()
            .subscribe(data => {
            this.screenMessage = data;
        });
        this.fetchRecoveryData();
    }
    fetchRecoveryData() {
        this.recoveryDataSubscription = this.accountInfoService
            .getAccountRecovery()
            .subscribe(data => {
            this.accountRecoveryData = data;
            this.addMobile = data.security.accountRecoveryInfo.mobile
                ? false
                : true;
            this.addEmail = data.security.accountRecoveryInfo.email ? false : true;
            this.mobile = !this.addMobile
                ? this.accountInfoService.formatPhoneNumber(data.security.accountRecoveryInfo.mobile)
                : '';
        });
    }
    updateContent(type) {
        type === 'mobile'
            ? this.router.navigateByUrl('/settings/account-and-personal-info/account-info/edit-account-recovery/add-update-mobile')
            : this.router.navigateByUrl('/settings/account-and-personal-info/account-info/edit-account-recovery/add-update-email');
    }
    goBack() {
        this.router.navigateByUrl('/settings/account-and-personal-info');
    }
    ngOnDestroy() {
        this.moreContentSubscription.unsubscribe();
        this.recoveryDataSubscription.unsubscribe();
    }
};
