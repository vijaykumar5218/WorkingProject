let EditPasswordPage = class EditPasswordPage {
    constructor(headerType, router, accountInfoService, modalController) {
        this.headerType = headerType;
        this.router = router;
        this.accountInfoService = accountInfoService;
        this.modalController = modalController;
        this.displayText = _constants_displayText_json__WEBPACK_IMPORTED_MODULE_6__;
        this.showPassword = false;
        this.showConfirmPassword = false;
        this.showNewPassword = false;
        this.submitDisabled = true;
        this.actionOption = {
            headername: this.displayText.actionOption.headername,
            btnleft: true,
            btnright: true,
            buttonLeft: {
                name: '',
                link: this.displayText.actionOption.buttonLeft,
            },
            buttonRight: {
                name: '',
                link: this.displayText.actionOption.buttonRight,
            },
        };
    }
    ionViewWillEnter() {
        this.headerType.publish({
            type: _shared_lib_constants_headerType_enum__WEBPACK_IMPORTED_MODULE_4__.HeaderType.navbar,
            actionOption: this.actionOption,
        });
        this.fetchRecoveryData();
    }
    routeToNotification() {
        this.router.navigateByUrl('/settings/notification-settings');
    }
    goBack() {
        this.router.navigateByUrl('/settings/account-and-personal-info');
    }
    toggleShowPassword(password) {
        switch (password) {
            case 'old':
                this.showPassword = !this.showPassword;
                break;
            case 'new':
                this.showNewPassword = !this.showNewPassword;
                break;
            case 'confirm':
                this.showConfirmPassword = !this.showConfirmPassword;
                break;
        }
    }
    fetchRecoveryData() {
        this.recoveryDataSubscription = this.accountInfoService
            .getAccountRecovery()
            .subscribe(data => {
            this.accountRecoveryData = data;
        });
    }
    passwordChanged(val) {
        this.password = val;
    }
    submitNewPassword() {
        return (0,tslib__WEBPACK_IMPORTED_MODULE_7__.__awaiter)(this, void 0, void 0, function* () {
            const modal = yield this.modalController.create({
                component: _shared_lib_components_alert_alert_component__WEBPACK_IMPORTED_MODULE_2__.AlertComponent,
