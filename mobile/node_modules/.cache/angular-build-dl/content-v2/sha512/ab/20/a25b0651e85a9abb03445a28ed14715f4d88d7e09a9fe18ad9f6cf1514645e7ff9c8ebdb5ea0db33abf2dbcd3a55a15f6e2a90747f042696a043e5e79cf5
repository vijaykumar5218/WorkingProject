                swipeToClose: false,
                componentProps: {
                    titleMessage: this.displayText.alert.message,
                    imageUrl: this.displayText.alert.imageUrl,
                },
            });
            modal.onDidDismiss().then(() => {
                this.router.navigateByUrl('/settings/account-and-personal-info');
            });
            return modal.present();
        });
    }
};
