let AddUpdateEmailPage = class AddUpdateEmailPage {
    constructor(headerType, accountInfoService, router) {
        this.headerType = headerType;
        this.accountInfoService = accountInfoService;
        this.router = router;
        this.displayText = _constants_displayText_json__WEBPACK_IMPORTED_MODULE_5__;
        this.actionOption = {
            headername: '',
            btnleft: true,
            btnright: true,
            buttonLeft: {
                name: '',
                link: this.displayText.actionOption.buttonLeft,
            },
            buttonRight: {
                name: '',
                link: this.displayText.actionOption.buttonRight,
            },
        };
    }
    ngOnInit() {
        this.headerType.publish({
            type: _shared_lib_constants_headerType_enum__WEBPACK_IMPORTED_MODULE_3__.HeaderType.navbar,
            actionOption: this.actionOption,
        });
        this.fetchRecoveryData();
    }
    fetchRecoveryData() {
        this.recoveryDataSubscription = this.accountInfoService
            .getAccountRecovery()
            .subscribe(data => {
            this.accountRecoveryData = data;
            this.addEmail = this.accountRecoveryData.security.accountRecoveryInfo
                .email
                ? false
                : true;
            this.actionOption.headername = this.addEmail
                ? this.displayText.actionOption.updateHeader
                : this.displayText.actionOption.addHeader;
            this.headerType.publish({
                type: _shared_lib_constants_headerType_enum__WEBPACK_IMPORTED_MODULE_3__.HeaderType.navbar,
                actionOption: this.actionOption,
            });
        });
    }
    valueChanged(val) {
        this.email = val;
    }
    goBack() {
        this.router.navigateByUrl('/settings/account-and-personal-info/account-info/edit-account-recovery');
    }
    ngOnDestroy() {
        this.recoveryDataSubscription.unsubscribe();
    }
};
